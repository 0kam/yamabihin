{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.7.1",
    "@chakra-ui/utils": "^2.0.15",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.6.7",
    "framer-motion": "^10.12.18",
    "msw": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prestart": "rimraf public/mockServiceWorker.js && copyfiles -f node_modules/msw/lib/mockServiceWorker.js public/",
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@chakra-ui|@emotion|framer-motion|axios)/)\"",
    "eject": "react-scripts eject",
    "msw:init": "msw init public/ --save"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@types/node": "^22.13.11",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "identity-obj-proxy": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
